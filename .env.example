# GitHub Configuration
NEXT_PUBLIC_GITHUB_USER=your_github_username
NEXT_PUBLIC_GITHUB_TOKEN=your_github_token

# Vercel Configuration
# Get your token from: https://vercel.com/account/tokens
# Create a token with read access to projects and deployments
NEXT_PUBLIC_VERCEL_TOKEN=your_vercel_token
# Optional: Only needed if using a team account
NEXT_PUBLIC_VERCEL_TEAM_ID=your_team_id_optional
